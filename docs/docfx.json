{
  "metadata": [
    {
      "src": [
        {
          "src": "../Assets/Danmokou/Plugins/Self",
          "files": [
            "Danmaku/Entities/Beh/**.cs",
            "Danmaku/Utils/**.cs",
            "Core/DataStructures.cs",
            "Danmaku/StateMachines/**.cs",
            "Danmaku/BulletDescriptors/**.cs",
            "Danmaku/BulletManagement/**.cs",
			"Danmaku/Patterning/**.cs",
            "Danmaku/Movement.cs",
			"Danmaku/Enums.cs",
            "Danmaku/Rendering/CurvedTileRenderLaser.cs",
			"Core/Services/Events.cs",
			"Core/Enums.cs",
			"Core/Attributes.cs"
          ]
        }
      ],
      "dest": "api",
      "filter": "filter.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "license/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
	"globalMetadata": {
		"_enableSearch": "true"
	},
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
	  "templates/dmk"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}